<h2>用户管理</h2>

<table class="table">
  <tr>
    <th>ID</th>
    <th>Email</th>
    <th>Actions</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= link_to "edit", edit_admin_user_path(user), :class => "btn btn-default" %></td>
    </tr>
  <% end %>
</table>
